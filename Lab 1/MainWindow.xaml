<Window x:Class="Lab_1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Lab_1"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Label x:Name="fnLabel" Content="First Name" HorizontalAlignment="Left" Margin="300,125,0,0" VerticalAlignment="Top" Width="100"/>
        <Label x:Name="lnLabel" Content="Last Name" HorizontalAlignment="Left" Margin="300,150,0,0" VerticalAlignment="Top" Width="100"/>
        <Button x:Name="submitButton" Content="Submit" HorizontalAlignment="Left" Margin="350,225,0,0" VerticalAlignment="Top" Width="75" Click="submitButton_Click"/>
        <TextBox x:Name="fnTextBox" HorizontalAlignment="Left" Height="23" Margin="400,128,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="100"/>
        <TextBox x:Name="lnTextBox" HorizontalAlignment="Left" Height="23" Margin="400,153,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="100"/>

    </Grid>
</Window>
